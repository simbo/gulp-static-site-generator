    "darwin",
    "linux"
  ],
  "files": [
    "index.js",
    "LICENSE",
    "README.md"
  ],
  "dependencies": {
    "gray-matter": "^4.0.2",
    "gulp-util": "^3.0.7",
    "highlight.js": "^10.5.0",
    "jade": "^1.11.0",
    "marked": "^1.2.8",
    "merge": "^2.1.0",
    "slug": "^4.0.2",
    "through2": "^4.0.2"
  },
  "devDependencies": {
    "codecov": "^3.8.1",
    "eslint": "^7.18.0",
    "eslint-config-simbo": "^1.5.0",
    "gulp": "^4.0.2",
    "istanbul": "^0.4.2",
    "mocha": "^8.2.1",
    "stream-assert": "^2.0.3"
  }
